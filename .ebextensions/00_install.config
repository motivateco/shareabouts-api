option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: wsgi.py
commands:
  00_install_gdal:
    command: 'yum install -y --enablerepo=epel gdal'
  01_collect_static:
    command: 'source /opt/python/run/venv/bin/activate && python src/manage.py collectstatic --noinput'
packages:
  yum:
    postgresql93-server: []
    postgresql93-devel: []
    git: []
